                          <a href="#" class="font-bold">Ultimos Servicios</a>
                        </div>
                         <section class="panel-body slim-scroll" data-height="350" data-size="10px">
                        <ul class="list-group list-group-lg no-bg auto">                          
                    
                         <% for(var i=0; i < data.eventosCompletados.length; i++) { %>
                     
                          <a href="#" class="list-group-item clearfix">
                            <span class="pull-left thumb-sm avatar m-r">
                              <img src="<%- data.eventosCompletados[i].dataDriver.driverPicture %>" alt="...">
                                <% if (data.eventosCompletados[i].exp == "positivo") {%>
                              <i class="on b-white bottom"></i>
                              <%  } %>
                               <% if (data.eventosCompletados[i].exp == "negativo") {%>
                              <i class="busy b-white bottom"></i>
                              <%  } %>
                              <% if (data.eventosCompletados[i].exp == null ) {%>
                              <i class="away b-white bottom"></i>
                              <%  } %>

                             
                            </span>
                            <span class="text-muted m-l-sm pull-right">
                            <i class="fa fa-clock-o"></i>
                             <span data-livestamp="<%-  moment(data.eventosCompletados[i].createdAt)  %>"></span>
                          </span>
                            <span class="clear">
                              <span><%- data.eventosCompletados[i].dataDriver.driverName %> <%- data.eventosCompletados[i].dataDriver.driverLastname %></span>
                              <small class="text-muted clear text-ellipsis"><%- data.eventosCompletados[i].eventSector%> - <%- data.eventosCompletados[i].eventCalle%> </small>
                              <small class="text-muted clear text-ellipsis"><%- data.eventosCompletados[i].passengerName%> <%- data.eventosCompletados[i].passengerLastname%> - "<%- data.eventosCompletados[i].coment%>" </small>
                           
                            </span>
                          </a>

                           <%  } %> 

